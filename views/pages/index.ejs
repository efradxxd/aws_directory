<html>

<head>
    <title>Team 1 Directory</title>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <!--[if lte IE 8]><script src="assets/js/ie/html5shiv.js"></script><![endif]-->
    <link rel="stylesheet" href="/css/main.css" />
    <!--[if lte IE 8]><link rel="stylesheet" href="/css/ie8.css" /><![endif]-->
    <!--[if lte IE 9]><link rel="stylesheet" href="/css/ie9.css" /><![endif]-->
</head>

<body>
    <div id="page-wrapper">
        <!-- Header -->
        <div id="header">
            <nav id="nav">
                <ul>
                    <li class="current"><a href="index.html">Home</a></li>
                </ul>
            </nav>
            <!-- Logo -->
            <h1 id="g-tec-logo"><a href="index.html" id="logo">Cloud Computing <em>by G-TEC</em></a></h1>

            <!-- Nav -->


        </div>

        <!-- Gigantic Heading -->
        <section id="content-directory" class="wrapper style2">
            <div class="container">
                <header class="major">
                    <input type="text" id="filterInput" onkeyup="filterMembers()" placeholder="Search for names..">
                    <h1>Members</h1>
                    <table id="member-table">
                        <tr class="header-table">
                            <th style="width:20%;">Image</th>
                            <th style="width:20%;">Name</th>
                            <th style="width:40%;">Confluence</th>
                        </tr>
                        <% teamMembers.forEach(function(member) { %>
                        <tr>
                            <td>
                                <img class="prof-img" src="<%= member.Img %>" alt="">
                            </td>
                            <td>
                                <%= member.FirstName %> <%= member.LastName %>
                            </td>
                            <td>
                                <a href="<%= member.Profile %>">Go to profile</a>
                            </td>

                        </tr>
                        <% }); %>

                    </table>
                    <ul>


                    </ul>
                </header>
            </div>
        </section>

        <!-- Footer -->
        <div id="footer">
            <!-- Icons -->
            <ul class="icons">
                <li><a href="https://twitter.com/Marcore_JS" class="icon fa-twitter"><span class="label">Twitter</span></a></li>
                <li><a href="https://github.com/g-tecio" class="icon fa-github"><span class="label">GitHub</span></a></li>
                <li><a href="https://www.linkedin.com/in/marco-antonio-mercado-lugo-84bb4211b/" class="icon fa-linkedin"><span
                            class="label">LinkedIn</span></a></li>
            </ul>

            <!-- Copyright -->
            <div class="copyright">
                <ul class="menu">
                    <li>&copy; G-Tec. All rights reserved</li>
                    <li>Design: <a href="http://html5up.net">HTML5 UP</a></li>
                </ul>
            </div>

        </div>

    </div>

    <!-- Scripts -->
    <script src="/js/jquery.min.js"></script>
    <script src="/js/jquery.dropotron.min.js"></script>
    <script src="/js/skel.min.js"></script>
    <script src="/js/util.js"></script>
    <!--[if lte IE 8]><script src="/js/ie/respond.min.js"></script><![endif]-->
    <script src="/js/main.js"></script>
    <script src="/js/script.js"></script>

</body>

</html>